import java.util.*;
class main
{
public static void main(String args[])
{
int num=5;
int row,col;
for(row=1;row<=num;System.out.printf("\n"),row+=1)
{
for(col=1;col<=num;col+=1)
{
if(row==1 || col==1 || row==num || col==num || row==col || row+col==num+1)
System.out.print(" * ");
else System.out.print(" ");
}
}
}
}
