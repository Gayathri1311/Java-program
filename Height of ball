import java.util.*;
class Main
{
    public static void main(String args[])
    {
        Scanner obj=new Scanner(System.in);
        int H=obj.nextInt();
        int V=obj.nextInt();
        int Fv=obj.nextInt();
        int en=V/Fv;
        int h1=H*(en*en);
        System.out.print(h1);
       
        
    }

    
}
