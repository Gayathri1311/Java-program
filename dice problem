import java.util.*;
class Main
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int count=0;
        int sum=sc.nextInt();;
        if(sum<12||sum>2)
        {
       
        for(int i=1;i<=6;i++)
        {
            if((sum-i)<=6&&(sum-i)!=0)
            {
                System.out.println((sum-i)+","+i);
                 count++;
            }
        }

        System.out.println(count);
        }
        
        
    }
}
