import java.util.*;
class Main
{
    public static void main(String args[])

    {
        Scanner obj=new Scanner(System.in);
        int a=obj.nextInt();
        int b=obj.nextInt();
        int c=a*b;
        if(c>12)
        {
            int d=c-12;
            System.out.print(d);
        }
        else
        {
            System.out.print(c);
        }
    }
}
