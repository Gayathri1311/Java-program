import java.util.*;
class Main
{
    public static void main(String args[])
    {
        Scanner obj=new Scanner(System.in);
        int a=obj.nextInt();
        int[] arr=new int[a];
        int count =0;
        for(int i=0;i<a;i++)
        {
            
            arr[i]=obj.nextInt();
            if(arr[i]<0)
            {
                count++;
            }
        }
        System.out.println(count);
    }
}
